@model IEnumerable<Kintai.Models.AttendanceMonth>

@{
    ViewBag.Title = "Index";
}

<h2>勤怠管理</h2>

<p>
    @Html.ActionLink("新規月入力", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Month)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Month)
        </td>
        <td>
            @Html.ActionLink("編集", "Edit", new { id=item.Month }) 
        </td>
    </tr>
}

</table>
